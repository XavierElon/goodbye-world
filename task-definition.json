{
  "family": "goodbye-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::275136276893:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "goodbye-world-container",
      "image": "275136276893.dkr.ecr.us-west-1.amazonaws.com/goodbye-world:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/goodbye-task",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
