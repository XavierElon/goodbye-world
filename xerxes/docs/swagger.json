{
    "swagger": "2.0",
    "info": {
        "description": "A simple Go API with health and goodbye endpoints",
        "title": "Go API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:10471",
    "basePath": "/",
    "paths": {
        "/goodbyeworld": {
            "get": {
                "description": "Returns a goodbye message",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "goodbye"
                ],
                "summary": "Goodbye world endpoint",
                "responses": {
                    "200": {
                        "description": "Goodbye, World!",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "Returns the health status of the API",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "text/plain"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Health check endpoint",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}